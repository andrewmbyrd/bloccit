<h1>Sign Up Confirmation</h1>


    <h3>Are you sure these values are correct?</h3>
      <p>Email:<%=@user.email %></p>
      <p>Name: <%=@user.name %></p>


<div class = "side-to-side">
  <%= button_to "Yes", {controller: "users", action: "create", params: params[:user]}, class: 'btn btn-success green' %>
  <%= link_to "No", new_user_path, class: 'btn btn-failure gray' %>
</div>
